<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:selectors="clr-namespace:AppListaTareas.Selector"
             x:Class="AppListaTareas.MVVM.View.DataView"
             Title="DataView">
    <ContentPage.Resources>
        <selectors:ToDoDataTemplateSelector x:Key="ToDoStyleSelector"/>
    </ContentPage.Resources>
    <CollectionView ItemsSource="{Binding ToDo}"
                    ItemTemplate="{StaticResource ToDoStyleSelector}">
        
    </CollectionView>
</ContentPage>